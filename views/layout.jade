doctype 5
html
  head
    title= title
    link(rel='stylesheet', href='/stylesheets/style.css')
    link(rel='stylesheet', href='/stylesheets/bootstrap.css')
    script(type='text/javascript', src='/javascripts/jquery-1.8.1.js')
    script(type='text/javascript', src='/javascripts/bootstrap.js')
    block head
  body
    div.navbar
      div.navbar-inner
        a.brand #{title}
        ul.nav
          li.active(id='liHome') 
            a(href='/') Home
          li 
            a(href='#', id='lnkHintsModal') Hints
          li(id='liLogout')
            a(href='/logout') Logout
    block content